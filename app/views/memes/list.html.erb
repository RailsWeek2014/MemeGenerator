<div class="page-header">
  <h1>Meine Memes</h1>
</div>
<div align="right">
<a href="<%= newmeme_path %>" class="btn btn-primary btn-lg"><span class="glyphicon glyphicon-plus-sign"></span> Neues Meme</a>
</div>
<br />
<% if @memes.all.count == 0 %>
<%= "Du hast keine Memes erstellt" %>
<% end %>
<div class="row">
<%= render partial: "memes/meme", collection: @memes %>
</div>
<%= paginate @memes %>