<!DOCTYPE html>
<html>
<head>
    <title>MemeGenerator</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
</head>
<body>
    <div id="container">
        <div id="header">
            <h1>MemeGenerator</h1>
        </div>
        <div id="navigation">
          <%= render :partial => 'layouts/navigation' %>
        </div>
        <div id="content">

            <%= yield %>
        </div>
    </div>
</body>
</html>
