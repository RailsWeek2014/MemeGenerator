    <tr>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td>
      <% if user.isadmin == true %>
      Ja
      <% else %>
      Nein
      <% end %>
      </td>
      <td><%= user.created_at %></td>
      <td><%= user.last_sign_in_at %></td>
      <td><%= link_to "destroy", acp_user_path(user), method: :delete, class:"btn btn-danger" %></td>
      <td><%= link_to "edit", edit_acp_user_path(user), class:"btn btn-primary" %></td>
    </tr>