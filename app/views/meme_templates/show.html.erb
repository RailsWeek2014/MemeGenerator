<div class="page-header">
  <h1><%= @meme_template.title %> </h1>
</div>
<p>
<b>Erstellt von</b>
<% if @meme_template.user_id != nil && User.exists?(:id => @meme_template.user_id) %>
<% @User = User.find(@meme_template.user_id) %>
<%= @User.username %>
<% else %>
unregistriert
<% end %>
</p>
<p>
<b>Erstellt am</b>
<%= @meme_template.created_at %>
</p>
<p>
<p>
<b>privates Template:</b> 
<% if @meme_template.isprivate == true %>
<%= "Ja" %>
<% else %>
<%= "Nein" %>
<% end %>
</p>
<div align="center">
<%= image_tag @meme_template.template_img.to_s %>  </p>
</div>
</p>